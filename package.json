{
  "name": "fg-cw-backend",
  "version": "0.0.0",
  "description": "fg-cw-backend",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "docker:dev": "NODE_ENV=development npm run server:watch",
    "dev": "NODE_ENV=development npm run server:watch",
    "dev:debug": "NODE_ENV=development npm run server:debug",
    "format": "prettier --write \"**/*.{cjs,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{cjs,js,json,md}\"",
    "git:pre-commit-hook": "npm run format:check && npm run lint",
    "postinstall": "npm run setup:husky",
    "lint": "eslint --cache --cache-strategy content \"**/*.{cjs,js}\"",
    "lint:fix": "npm run lint -- --fix",
    "postversion": "git add package.json package-lock.json && git commit -m $npm_package_version",
    "test": "vitest",
    "test:integration": "vitest --config ./test/vitest.config.js",
    "coverage": "vitest run --coverage",
    "server:watch": "nodemon --inspect=0.0.0.0 --ext js,json --legacy-watch ./src",
    "server:debug": "nodemon --inspect-brk=0.0.0.0 --ext js,json --legacy-watch ./src",
    "start": "NODE_ENV=production node --use-strict .",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module"
  },
  "lint-staged": {
    "**/*.js": "npm run lint",
    "*": "prettier --ignore-unknown --list-different --write"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@aws-sdk/client-sqs": "3.787.0",
    "@aws-sdk/client-sns": "3.787.0",
    "@defra/hapi-tracing": "1.0.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "10.0.1",
    "@hapi/hapi": "21.4.0",
    "@hapi/inert": "^7.1.0",
    "@hapi/vision": "^7.0.3",
    "@hapi/wreck": "^18.1.0",
    "aws-embedded-metrics": "4.2.0",
    "aws4": "1.13.2",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "global-agent": "3.0.0",
    "hapi-pino": "12.1.0",
    "hapi-pulse": "3.0.1",
    "hapi-swagger": "^17.3.2",
    "https-proxy-agent": "7.0.6",
    "migrate-mongo": "12.1.3",
    "mongo-locks": "3.0.2",
    "mongodb": "6.15.0",
    "node-fetch": "3.3.2",
    "pino": "9.6.0",
    "pino-pretty": "13.0.0",
    "undici": "7.7.0"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@shelf/jest-mongodb": "5.1.0",
    "@vitest/coverage-v8": "3.1.1",
    "eslint": "9.27.0",
    "eslint-config-prettier": "10.1.1",
    "husky": "9.1.7",
    "joi": "17.13.3",
    "lint-staged": "^15.5.0",
    "neostandard": "0.12.1",
    "nodemon": "3.1.9",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "testcontainers": "^10.24.0",
    "vitest": "3.1.1"
  }
}
